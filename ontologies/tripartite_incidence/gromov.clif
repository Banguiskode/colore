
(cl-text http://colore.oor.net/tripartite_incidence/gromov.clif

(cl-imports http://colore.oor.net/bipartite_incidence/definitions/bipartite_def.clif)

(cl-imports http://colore.oor.net/tripartite_incidence/simplicial_complex.clif)

(forall (l1 l2 l3)
	(if	(and	(intersect l1 l2)
			(intersect l1 l3)
			(intersect l2 l3))
		(exists (q)
			(and	(plane q)
				(in l1 q)
				(in l2 q)
				(in l3 q)))))

)
