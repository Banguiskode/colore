
(cl-text http://colore.oor.net/tripartite_incidence/thin_plane_line_intersect.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(cl-imports http://colore.oor.net/bipartite_incidence/definitions/bipartite_def.clif)

(forall (l1 l2 q)
	(if	(and	(line l1)
			(line l2)
			(not (= l1 l2))
			(plane q)
			(in l1 q)
			(in l2 q))
		(exists (l3)
			(and	(line l3)
				(in l3 q)
				(intersect l1 l3)))))

)
