
(cl-text http://colore.oor.net/tripartite_incidence/coplanar_collinear.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (p q)
	(if	(and	(plane q)
			(point p)
			(in p q))
		(exists (p1 p2)
			(and	(collinear p p1)
				(collinear p p2)
				(not (= p1 p2))
				(in p1 q)
				(in p2 q)))))

)
