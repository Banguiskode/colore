
(cl-text http://colore.oor.net/tripartite_incidence/coplanar_collinear.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(cl-imports http://colore.oor.net/tripartite_incidence/definitions/tripartite_def.clif)

(forall (p q)
	(if	(and	(plane q)
			(point p)
			(in p q))
		(exists (p1 p2)
			(and	(plane_collinear p p1 q)
				(plane_collinear p p2 q)
				(not (= p1 p2))
				(in p1 q)
				(in p2 q)))))

)
