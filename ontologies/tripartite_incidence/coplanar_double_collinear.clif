
(cl-text http://colore.oor.net/tripartite_incidence/coplanar_double_collinear.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(cl-imports http://colore.oor.net/tripartite_incidence/definitions/tripartite_def.clif)

(forall (x y z w q)
	(if	(and	(plane q)
			(in x q)
			(in y q)
			(in z q)
			(in w q)
			(plane_collinear x y q)
			(plane_collinear x z q)
			(plane_collinear x w q))
		(or	(= w z)
			(= y z)
			(= w y))))

)
