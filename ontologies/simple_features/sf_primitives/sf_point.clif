(cl-text http://colore.oor.net/simple_features/sf_primitives/sf_point.clif

(cl-imports http://colore.oor.net/simple_features/sf_primitives/sf_geometry.clif)
(cl-imports http://colore.oor.net/multidim_space_codi/definitions/point.clif)

(cl-comment 'Simple Feature Point is a subclass of CODI Point')
(forall (x)
	(iff	
		(sf_point x)
		(Point x)
	)
)

(cl-comment 'Simple Feature Point is a subclass of Simple Feature Geometry')
(forall (x)
	(if	(sf_point x)
		(sf_geometry x)
	)
)
)