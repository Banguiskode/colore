(cl-text http://colore.oor.net/simple_features/sf_primitives/sf_geometry_collection.clif

(cl-imports http://colore.oor.net/simple_features/sf_primitives/sf_geometry.clif)
(cl-imports http://colore.oor.net/multidim_space_codib/definitions/multipart_s.clif)

(cl-comment 'Simple Feature Geometry Collection is a subclass of Simple Feature Geometry')


(forall (x)
	(if	
		(sf_geometry_collection x)
		(sf_geometry x)
	)
)


(forall (x)
	(iff	
		(sf_geometry_collection x)
		(Multipart_S x)
	)
)
)