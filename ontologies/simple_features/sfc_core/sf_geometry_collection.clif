(cl-text http://colore.oor.net/simple_features/sfc_core/sf_geometry_collection.clif


(cl-imports http://colore.oor.net/simple_features/sfc_core/definitions/sf_geometry.clif)


(cl-comment 'SFC-A6')
(forall (x)
	(if
		(sf_geometry_collection x)
		(and
			(not (sf_point x))
			(not (sf_curve x))
			(not (sf_surface x))
		)
	)
)

(cl-comment 'SFC-A11')
(forall (x)
	(iff
		(sf_geometry_collection x)
		(or
			(sf_multi_point x)
			(sf_multi_curve x)
			(sf_multi_surface x)
		)
	)
)

)