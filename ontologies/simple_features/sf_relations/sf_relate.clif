(cl-text http://colore.oor.net/simple_features/relations/sf_relate.clif

(cl-imports http://colore.oor.net/simple_features/relations/sf_equals.clif)
(cl-imports http://colore.oor.net/simple_features/relations/sf_touches.clif)
(cl-imports http://colore.oor.net/simple_features/relations/sf_crosses.clif)
(cl-imports http://colore.oor.net/simple_features/relations/sf_within.clif)
(cl-imports http://colore.oor.net/simple_features/relations/sf_overlaps.clif)
(cl-imports http://colore.oor.net/simple_features/relations/sf_contains.clif)
(cl-imports http://colore.oor.net/simple_features/relations/sf_intersects.clif)




(forall (x y)
	(if	
		(sf_relate x y)
		(or
			(sf_equals x y)
			(sf_touches x y)
			(sf_crosses x y)
			(sf_within x y)
			(sf_overlaps x y)
			(sf_contains x y)
			(sf_intersects x y)
		)
	)
)

)