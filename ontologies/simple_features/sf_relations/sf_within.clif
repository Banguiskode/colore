(cl-text http://colore.oor.net/simple_features/sf_relations/sf_within.clif

(cl-imports http://colore.oor.net/simple_features/sf_primitives/sf_geometry.clif)
(cl-imports http://colore.oor.net/multidim_space_codi/definitions/epp.clif)



(forall (x y)
	(if	
		(sf_within x y)
		(and
			(sf_geometry x)
			(sf_geometry y)
			(PP x y)
		)
	)
)

)