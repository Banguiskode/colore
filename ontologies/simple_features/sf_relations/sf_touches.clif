(cl-text http://colore.oor.net/simple_features/relations/sf_touches.clif

(cl-imports http://colore.oor.net/simple_features/layer_1_sf_codi/sf_geometry.clif)
(cl-imports http://colore.oor.net/simple_features/layer_2_sf_sf/sf_polygon_sf.clif)

(cl-imports http://colore.oor.net/multidim_space_codi/definitions/sc.clif)



(forall (x y)
	(if	
		(sf_touches x y)
		(and
			(sf_geometry x)
			(sf_geometry y)
			(SC x y)
			(if
				(sf_polygon x)
				(not (sf_polygon y))
			)
		)
	)
)

)