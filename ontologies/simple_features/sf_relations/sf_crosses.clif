(cl-text http://colore.oor.net/simple_features/relations/sf_crosses.clif

(cl-imports http://colore.oor.net/simple_features/layer_1_sf_codi/sf_geometry.clif)
(cl-imports http://colore.oor.net/multidim_space_codi/definitions/inc.clif)



(forall (x y)
	(if	
		(sf_crosses x y)
		(and
			(sf_geometry x)
			(sf_geometry y)
			(Inc x y)
		)
	)
)

)