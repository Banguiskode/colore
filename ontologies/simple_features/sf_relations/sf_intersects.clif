(cl-text http://colore.oor.net/simple_features/relations/sf_intersects.clif

(cl-imports http://colore.oor.net/simple_features/layer_1_sf_codi/sf_geometry.clif)
(cl-imports http://colore.oor.net/simple_features/relations/sf_within.clif)



(forall (x y)
	(if	
		(sf_intersects x y)
		(and
			(sf_geometry x)
			(sf_geometry y)
			(not (sf_disjoint x y))
		)
	)
)

)