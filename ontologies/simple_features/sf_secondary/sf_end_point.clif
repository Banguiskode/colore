(cl-text http://colore.oor.net/simple_features/sf_secondary/sf_end_point.clif

(cl-imports http://colore.oor.net/simple_features/sf_primitives/sf_point.clif)
(cl-imports http://colore.oor.net/simple_features/sf_primitives/sf_curve.clif)
(cl-imports http://colore.oor.net/multidim_space_codib/codi_bcont.clif)

(cl-comment 'Simple Feature End Point')
(forall (x y)
	(if	
		(sf_end_point x y)
		(and
			(sf_point x)
			(sf_curve y)
			(BCont x y)
		)
	)
)
)