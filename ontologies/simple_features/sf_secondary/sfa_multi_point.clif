(cl-text http://colore.oor.net/simple_features/sf_secondary/sfa_multi_point.clif


(cl-imports http://colore.oor.net/simple_features/sf_primitives/sfa_geometry_collection.clif)
(cl-imports http://colore.oor.net/simple_features/sfa_primitives.clif)
(cl-imports http://colore.oor.net/multidim_space_codi/definitions/point_region.clif)




(cl-comment 'Simple Feature Multipoint is a PointRegion')
(forall (x)
	(iff	
		(sfa_multi_point x)
		(PointRegion x)
	)
)

(forall (x)
	(if	
		(sfa_multi_point x)
		(and
			(sfa_geometry_collection x)
			(forall (y)
				(PP y x)
				(sfa_point y)
			)
		)
	)
)
)