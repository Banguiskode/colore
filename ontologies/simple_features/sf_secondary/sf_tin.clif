(cl-text http://colore.oor.net/simple_features/sf_secondary/sf_tin.clif

(cl-imports http://colore.oor.net/multidim_space_codib/definitions/simple_region.clif)
(cl-imports http://colore.oor.net/multidim_space_codi/definitions/areal_region.clif)
(cl-imports http://colore.oor.net/simple_features/sf_secondary/sf_triangle.clif)
(cl-imports http://colore.oor.net/simple_features/sf_secondary/sf_polyhedral_surface.clif)

(cl-comment 'Simple Feature TIN is a polyhedral surface whose parts are triangles')
(forall (x)
	(if	
		(sf_tin x)
		(and
			(sf_polyhedral_surface x)
			(forall (y)
				(if
					(PP y x)
					(sf_triangle y)
				)
			)
		)
	)
)
)