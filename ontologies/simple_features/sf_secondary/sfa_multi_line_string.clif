(cl-text http://colore.oor.net/simple_features/sf_secondary/sfa_multi_line_string.clif


(cl-imports http://colore.oor.net/simple_features/sf_primitives/sfa_geometry_collection.clif)
(cl-imports http://colore.oor.net/simple_features/sfa_primitives.clif)
(cl-imports http://colore.oor.net/multidim_space_codib/definitions/multipart_curve.clif)




(forall (x)
	(iff	
		(sfa_multi_line_string x)
		(Multipart_Curve x)
	)
)

(forall (x)
	(if	
		(sfa_multi_line_string x)
		(and
			(sfa_geometry_collection x)
			(forall (y)
				(PP y x)
				(sfa_line_string y)
			)
		)
	)
)
)