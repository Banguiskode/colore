
(cl-text http://colore.oor.net/quivers/quiver.clif

(forall (e x y)
	(if	(madj e x y)
		(and	(edge e)
			(vertex x)
			(vertex y))))

(forall (x)
	(if	(edge x)
		(not (vertex x))))
		
(forall (x)
	(if	(edge x)
		(exists (y z)
			(madj x y z))))

(forall (e x y z)
	(if	(and	(madj e x y)
			(modj e z y))
		(= x z)))

(forall (e x y z)
	(if	(and	(madj e x y)
			(modj e y z))
		(= y z)))

)
