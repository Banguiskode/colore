
(cl-text http://colore.oor.net/mereology/nonoverlaps.clif

(cl-imports http://colore.oor.net/mereology/m_mereology.clif)

(cl-imports http://colore.oor.net/mereology/definitions/ppart.clif)

(cl-imports http://colore.oor.net/mereology/definitions/overlaps.clif)

(forall (x y)
        (if     (ppart x y)
                (exists  (z)
			(not (overlaps z x)))))


)
