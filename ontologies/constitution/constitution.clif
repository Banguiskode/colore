(cl-text http://colore.oor.net/constitution/constitution.clif

(cl-imports http://colore.oor.net/matter/matter.clif)

(forall (x y)
        (if     (constitutes x y)
        	(and	(mat x)
			(MaterialObject x))))

(forall (x)
	(if	(MaterialObject x)
	(exists (y)
			(constitutes y x))))
			
(forall (x y1 y2)
	(if	(and	(mat x)
			(constitutes x y1)
			(constitutes x y2))
		(= y1 y2)))

(forall (x1 x2 y)
	(if	(and	(mat x1)
			(mat x2)
			(constitutes x1 y)
			(constitutes x2 y))
		(= x1 x2)))

)
