
(cl-text  http://colore.oor.net/combined_mereotopology/definitions/PO.clif

(cl-imports  http://colore.oor.net/combined_mereotopology/mt.clif)

(cl-imports  http://colore.oor.net/mereology/definitions/overlaps.clif)

%Proper Overlap
(forall (x y)
	(iff	(PO x y)
		(and	(overlaps x y)
			(not (part x y))
			(not (part y x)))))


)
