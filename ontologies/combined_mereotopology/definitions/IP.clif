
(cl-text  http://colore.oor.net/combined_mereotopology/definitions/IP.clif

(cl-imports  http://colore.oor.net/combined_mereotopology/mt.clif)

(cl-imports  http://colore.oor.net/mereology/definitions/overlaps.clif)

(forall (x y)
	(iff	(IP x y)
		(and	(part x y)
			(forall (z)
				(if	(C z x)
					(overlaps z y))))))


)
