
(cl-text http://colore.oor.net/orderings/definitions/dhm3.clif

(cl-imports http://colore.oor.net/orderings/definitions/incomparable.clif)

(cl-imports http://colore.oor.net/orderings/definitions/lt.clif)

(forall (x y z u)
	(iff	(dhm3 x y z u)
		(exists (w)
			(and	(lt w x)
				(lt w y)
				(lt w z)
				(incomparable w u)))))

)
