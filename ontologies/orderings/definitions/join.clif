
(cl-text http://colore.oor.net/orderings/definitions/join.clif

(cl-imports http://colore.oor.net/orderings/quasiorder.clif)

(forall (x y s) 
	(iff	
        (join x y s)
		(and 	
            (leq x s)
			(leq y s)
			(forall (z)
				(if	(and	
                        (leq x z)
						(leq y z)
                    )
					(leq s z))))))


)
