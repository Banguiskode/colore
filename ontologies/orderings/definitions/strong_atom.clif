
(cl-text http://colore.oor.net/orderings/definitions/strong_atom.clif

(cl-imports http://colore.oor.net/orderings/definitions/bottom.clif)

(cl-imports http://colore.oor.net/orderings/definitions/covers.clif)

(forall (x)
	(iff	(strong_atom x)
		(exists (y)
			(and	(bottom y)
				(covers x y)))))


)
