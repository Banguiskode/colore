
(cl-text http://colore.oor.net/orderings/definitions/strong_coatom.clif

(cl-imports http://colore.oor.net/orderings/definitions/top.clif)

(cl-imports http://colore.oor.net/orderings/definitions/covers.clif)

(forall (x)
	(iff	(strong_coatom x)
		(exists (y)
			(and	(top y)
				(covers y x)))))


)
