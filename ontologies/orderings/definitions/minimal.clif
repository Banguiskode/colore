
(cl-text http://colore.oor.net/orderings/definitions/minimal.clif

(cl-imports http://colore.oor.net/orderings/quasiorder.clif)

(cl-imports http://colore.oor.net/orderings/definitions/lt.clif)

(cl-comment 'Equivalent to bottom; either remove or at least restate in terms of leq')

(forall (x)
		(iff	(minimal x)
				(not	(exists (y)
								(lt y x)))))

)
