
(cl-text http://colore.oor.net/orderings/tarski_boolean.clif

(cl-imports http://colore.oor.net/orderings/partial_ordering.clif)

(cl-imports http://colore.oor.net/orderings/definitions/join.clif)

(cl-imports http://colore.oor.net/orderings/definitions/comp.clif)

(forall (x y)
	(exists (s)
		(join x y s)))

(forall (x)
	(exists (y)
		(comp x y)))

)
