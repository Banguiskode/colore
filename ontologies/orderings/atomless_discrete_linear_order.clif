
(cl-text http://colore.oor.net/orderings/atomless_discrete_linear_order.clif

(cl-imports http://colore.oor.net/orderings/bounded_linear_order.clif)

(forall (x y)
	(if	(and	(lt x y)
			(not (top y)))
		(exists (z)
			(and	(covers y z)
				(leq x z)))))

(forall (x y)
	(if	(and	(lt x y)
			(not (bottom x)))
		(exists (z)
			(and	(covers z x)
				(leq z y)))))

)
