
(cl-text http://colore.oor.net/orderings/atomless.clif

(cl-imports http://colore.oor.net/orderings/definitions/lt.clif)

(cl-imports http://colore.oor.net/orderings/definitions/bottom.clif)

(cl-imports http://colore.oor.net/orderings/definitions/top.clif)

(forall (x)
	(if	(not (bottom x))
		(exists (y)
			(and	(lt y x)
				(not (bottom y))))))
				
(forall (x y)
	(if	(and	(bottom x)
			(top y))
		(lt x y)))

)
