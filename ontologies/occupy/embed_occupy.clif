(cl-text http://colore.oor.net/occupy/embed_occupy.clif

(cl-imports http://colore.oor.net/occupy/occupy_root.clif)

(forall (x y r1 r2)
	(if	(and	(part r1 r2)
			(occupy x r1)
			(occupy y r)2)
		(physical_part x y)))

(forall (x y r1 r2)
	(if	(and	(C r1 r2)
			(occupy x r1)
			(occupy y r)2)
		(physical_C x y)))

)
