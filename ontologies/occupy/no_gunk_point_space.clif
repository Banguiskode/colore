(cl-text http://colore.oor.net/occupy/no_gunk_point_space.clif

(cl-imports http://colore.oor.net/occupy/occupy_root.clif)

(cl-imports http://colore.oor.net/occupy/definitions/occupy_def.clif)

(cl-imports http://colore.oor.net/mereology/definitions/mereology_def.clif)

(forall (x r)
	(if	(and	(physical_gunk x)
			(occupies x r))
		(gunky r)))

)
