
(cl-text http://colore.oor.net/occupy/theorems/graph_hom2occupy/Pi8.clif

(cl-imports http://colore.oor.net/occupy/subgraph/graph_hom.clif)

(forall (x)
	(iff	(physical_body x)
		(point x)))

(forall (x)
	(iff	(region x)
		(line x)))

(forall (x y)
	(iff	(spatial_C x y)
		(C x y)))

(forall (x y)
	(iff	(physical_C x y)
		(adj x y)))

(forall (x y)
	(iff	(occupies x y)
		(and	(in x y)
			(point x)
			(line y))))

)
