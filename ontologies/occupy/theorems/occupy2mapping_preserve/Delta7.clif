
(cl-text http://colore.oor.net/occupy/theorems/occupy2mapping_preserve/Delta7.clif

(cl-imports http://colore.oor.net/occupy/occupy_mereology/occupy_mereology.clif)

(forall (x)
	(iff	(point x)
		(physical_body x)))

(forall (x)
	(iff	(line x)
		(region x)))

(forall (x y)
	(iff	(part x y)
		(spatial_part x y)))

(forall (x y)
	(iff	(leq x y)
		(physical_part x y)))

(forall (x y)
	(iff	(in x y)
		(or	(occupies x y)
			(occupies y x)
			(= x y))))

)
