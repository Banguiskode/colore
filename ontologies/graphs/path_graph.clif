
(cl-text http://colore.oor.net/graphs/path_graph.clif

(cl-imports http://colore.oor.net/graphs/simple_graph.clif)

(cl-imports http://colore.oor.net/graphs/nobranch.clif)

(cl-imports http://colore.oor.net/graphs/definitions/graph_def.clif)

(exists (x)
	(leaf x))

(forall (x y z)
	(if	(and	(leaf x)
			(leaf y)
			(leaf z))
		(or	(= x y)
			(= x z)
			(= y z))))

)
