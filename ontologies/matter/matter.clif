(cl-text http://colore.oor.net/matter/matter.clif

(cl-imports http://colore.oor.net/constitution/constitution.clif)
(cl-import http://colore.oor.net/occupy/occupy_root.clif)

(forall (x y)
	(iff	(portionOf x y)
		(exists (w z r1 r2)
			(and	(constitutes w x)
				(constitutes z y)
				(occupies w r1)
				(occupies z r2)
				(region_part r1 r2)))))
				

)
