(cl-text http://colore.oor.net/matter/definitions/chunk_underlaps.clif

(cl-imports http://colore.oor.net/matter/matter.clif)

(cl-comment 'Underlaps for matter')
(forall (x y)
	(iff 	(chunk_underlaps x y)
		(exists (z)
                        (and
                                (chunkOf x z)
                                (chunkOf y z)))))

)
