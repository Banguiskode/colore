(cl-text http://colore.oor.net/matter/definitions/chunk_overlaps.clif

(cl-imports http://colore.oor.net/matter/matter.clif)

(cl-comment 'Overlaps for matter')
(forall (x y)
	(iff 	(chunk_overlaps x y)
		(exists (z)
                        (and
                                (chunkOf z x)
                                (chunkOf z y)))))

)
