(cl-text http://colore.oor.net/lattices/definitions/covers.clif

(cl-imports http://colore.oor.net/lattices/lattice.clif)

(cl-imports http://colore.oor.net/lattices/definitions/leq.clif)

(forall (x y)
	(iff
        (incomparable x y)
		(and
            (not (leq x y))
            (not (leq y x))
        )
    )
)

