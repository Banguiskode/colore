(cl-text http://colore.oor.net/lattices/weak_complemented_lattice.clif

(cl-imports http://colore.oor.net/lattices/bounded_lattice.clif)

(cl-imports http://colore.oor.net/lattices/definitions/lattice_defs.clif)

(forall (x y)
	(if	(not (leq y x))
		(exists (z)
			(and	(= (meet x z) zero)
				(not (= (meet y z) zero))))))

)
