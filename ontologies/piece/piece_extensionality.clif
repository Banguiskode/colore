(cl-text http://colore.oor.net/piece/piece_extensionality.clif)
(cl-imports http://colore.oor.net/piece/piece_def.clif)

(cl-text "Extensionality: if two objects are not identical, they have at least some uncommon piece.")

(forall (x y w)
        (if     (and    (properPieceOf w x)
                        (not (x=y))
                (exists (z)
                        (iff   (properPieceOf z x)
                               (not (properPieceOf z y)))))

)
