(cl-text http://colore.oor.net/piece/piece_dense.clif)
(cl-imports http://colore.oor.net/piece/piece_def.clif)

(cl-text "Dense Mereology")

(forall (x y)
        (if     (properPieceOf x y)
                (exists (z)
                        (and    (properPieceOf x z)
                                (properPieceOf z y))))

)
